
--# Main
-- Air Hockey

-- Main

supportedOrientations(LANDSCAPE_ANY)
displayMode(OVERLAY)
rectMode(RADIUS)
spriteMode(RADIUS)
ellipseMode(RADIUS)

function setup()
    
    -- Images
    button = Images:getButton()
    pauseButton = Images:getPauseButton()
    
    -- Neon Air Hockey
    parameter.watch("AirHockey_status")
    
    print("Air Hockey")
    print("- AirHockey_activate()")
    print("- AirHockey_deactivate()")
    
end

function touched(t)
    
    if AirHockey_status == "Active" then
        AirHockey_touched(t)
    end
    
end

function draw()
    
    background(0, 0, 0, 255)
    
    if AirHockey_status == "Active" then
        AirHockey_active()
        else AirHockey_status = "Inactive"
    end
    
end


--# Button
-- Button

Button = class()

function Button:init(name,x,y,width,height,fontsize,colourR,colourG,colourB,buttonImage,function0,function01,function02,function03,function04)
    
    self.name = name
    self.x = x
    self.y = y
    self.width = width
    self.height = height
    self.fontsize = fontsize
    self.colourR = colourR
    self.colourG = colourG
    self.colourB = colourB
    self.buttonImage = buttonImage
    self.function0 = function0 or nil
    self.function01 = function01 or nil
    self.function02 = function02 or nil
    self.function03 = function03 or nil
    self.function04 = function04 or nil
    
    self.pressed = "No"
    self.released = "No"
    self.tint = "No"
    
end

function Button:touched(t)
    if pressedField(self.x,self.y,self.width,self.height,t) == true then
        self.pressed = "Yes"
    end
    
    if self.pressed == "Yes" and t.x<=self.x+self.width and t.x>=self.x-self.width and t.y<=self.y+self.height and t.y>=self.y-self.height then
        self.tint = "Yes"
        else self.tint = "No"
    end
    
    if releasedField(self.x,self.y,self.width,self.height,t) == true and self.pressed == "Yes" then
        self.released = "Yes"
        self.pressed = "No"
    end
    
    if self.released == "Yes"
    then if self.function0 ~= nil then
            if self.function01 == nil
            then self.function0()
                else
                if self.function04 ~= nil
                then self.function0(self.function01,self.function02,self.function03,self.function04)
                end
                if self.function03 ~= nil
                then self.function0(self.function01,self.function02,self.function03)
                end
                if self.function02 ~= nil
                then self.function0(self.function01,self.function02)
                end
                if self.function01 ~= nil
                then self.function0(self.function01)
                end
            end
        end
    end
    
    self.released = "No"
    
    if t.state == ENDED then
        self.pressed = "No"
        self.tint = "No"
    end
    
end

function Button:draw()
    
    pushStyle()
    if self.tint == "Yes" then
        tint(self.colourR*1.5,self.colourG*1.5,self.colourB*1.5,225)
        elseif self.tint == "No" then
        tint(self.colourR,self.colourG,self.colourB,225)
    end
    sprite(self.buttonImage,self.x,self.y,self.width,self.height)
    fill(255, 255, 255, 255)
    fontSize(self.fontsize)
    text(self.name,self.x,self.y)
    popStyle()
    
end

--# Functions
-- Functions

-- Touch

function pressedField(x,y,width,height,t)
    if t.x <= x + width and t.x >= x - width and t.y <= y + height and t.y >= y - height and t.state == BEGAN
    then return true
    else return false end
end

function releasedField(x,y,width,height,t)
    if t.x <= x + width and t.x >= x - width and t.y <= y + height and t.y >= y - height and t.state == ENDED
    then return true
    else return false end
end


